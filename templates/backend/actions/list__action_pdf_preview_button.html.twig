{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') and object.document1FileName %}
    <a data-controller="pdf-button" data-action="pdf-button#clicked" data-toggle="modal" data-target="#pdf-preview-modal" data-pdf-button-attachment-value="{{ object.id }}" data-pdf-button-path-value="{{ vich_uploader_asset(object, 'document1File') }}" class="btn btn-sm btn-default" title="{{ 'back.action.pdf_preview' | trans }}">
        <i class="fa fa-file-pdf-o"></i>
    </a>
{% endif %}
