{% extends "@SonataAdmin/CRUD/base_list_field.html.twig" %}

{% block field %}
    {% if object.smallImage1FileName %}
        <img src="{{ vich_uploader_asset(object, 'smallImage1File') | imagine_filter('100x100') }}" alt="{{ object.year }}">
    {% else %}
        {{ constant('App\\Entity\\AbstractBase::DEFAULT_EMPTY_STRING') }}
    {% endif %}
{% endblock %}
