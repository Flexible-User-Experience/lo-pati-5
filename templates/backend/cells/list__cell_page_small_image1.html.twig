{% extends "@SonataAdmin/CRUD/base_list_field.html.twig" %}

{% block field %}
    {% if object.smallImage1FileName %}
        <img src="{{ vich_uploader_asset(object, 'smallImage1File') | imagine_filter('60x60') }}" alt="{{ object.name }}">
    {% elseif object.imageFileName %}
        <img src="{{ vich_uploader_asset(object, 'imageFile') | imagine_filter('Xx60') }}" alt="{{ object.name }}">
    {% else %}
        {{ constant('App\\Entity\\AbstractBase::DEFAULT_EMPTY_STRING') }}
    {% endif %}
{% endblock %}
