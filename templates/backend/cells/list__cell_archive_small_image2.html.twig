{% extends "@SonataAdmin/CRUD/base_list_field.html.twig" %}

{% block field %}
    {% if object.smallImage2FileName %}
        <img src="{{ vich_uploader_asset(object, 'smallImage2File') | imagine_filter('60x60') }}" alt="{{ object.year }}">
    {% else %}
        {{ constant('App\\Entity\\AbstractBase::DEFAULT_EMPTY_STRING') }}
    {% endif %}
{% endblock %}
