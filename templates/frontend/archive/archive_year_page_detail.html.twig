{% extends "frontend/base.html.twig" %}

{% block title %}{{ page.name }} 路 {{ archive.year }} 路 {{ menu }} 路 {{ parent() }}{% endblock %}

{% block main %}
    <main>
        {% if show_debug_page_info %}
            <div class="container">
                <div class="row my-5">
                    <div class="col">
                        <h1>{{ page }}</h1>
                        <hr>
                        <pre>P_ID# {{ page.id }} 路 PTT# '{{ page.templateTypeTransString | trans }}'</pre>
                    </div>
                </div>
            </div>
        {% endif %}
        {% include 'frontend/partials/page_template_types/default.html.twig' with {page: page} %}
    </main>
{% endblock %}

{% block footer_background_color_style_attribute %}{% endblock %}
